{
  "name": "psr.angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_ENV=development ng serve --disableHostCheck=true",
    "build-lib:authlib": "npm run build:lib --prefix ./projects/authlib/",
    "build-lib:corelib": "npm run build:lib --prefix ./projects/corelib/",
    "build-lib:headerlib": "npm run build:lib --prefix ./projects/headerlib/",
    "build-lib:myskilllib": "npm run build:lib --prefix ./projects/myskilllib/",
    "build-lib:sharedlib": "npm run build:lib --prefix ./projects/sharedlib/",
    "build-lib:skilllib": "npm run build:lib --prefix ./projects/skilllib/",
    "build-lib:skillmgtlib": "npm run build:lib --prefix ./projects/skillmgtlib/",
    "watch-lib:authlib": "npm run watch:lib --prefix ./projects/authlib/",
    "watch-lib:corelib": "npm run watch:lib --prefix ./projects/corelib/",
    "watch-lib:headerlib": "npm run watch:lib --prefix ./projects/headerlib/",
    "watch-lib:myskilllib": "npm run watch:lib --prefix ./projects/myskilllib/",
    "watch-lib:sharedlib": "npm run watch:lib --prefix ./projects/sharedlib/",
    "watch-lib:skilllib": "npm run watch:lib --prefix ./projects/skilllib/",
    "watch-lib:skillmgtlib": "npm run watch:lib --prefix ./projects/skillmgtlib/",
    "build": "cross-env NODE_ENV=production ng build",
    "watch": "cross-env NODE_ENV=production ng build --watch --configuration development",
    "test": "ng test",
    "goto:root": "cd ../../",
    "buildcss:dev": "postcss .",
    "package:lib": "ng-packagr -p ./projects/headerlib/ng-package.json",
    "package:play": "ng-packagr -p ./projects/play/ng-package.json",
    "build:sharedlib": "npm run before-build:lib --prefix ./projects/sharedlib && npm run build:lib --prefix ./projects/sharedlib",
    "build:headerlib": "npm run before-build:lib --prefix ./projects/headerlib && npm run build:lib --prefix ./projects/headerlib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.10",
    "@angular/compiler-cli": "~12.2.0",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.4.1",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "install-peers": "^1.0.3",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-scss": "^4.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "~4.3.5"
  }
}
