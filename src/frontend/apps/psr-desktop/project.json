{
    "projectType": "application",
    "root": "apps/psr-desktop",
    "sourceRoot": "apps/psr-desktop/src",
    "prefix": "psr",
    "targets": {
        "build": {
            "executor": "@angular-devkit/build-angular:browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/psr-desktop",
                "index": "apps/psr-desktop/src/index.html",
                "main": "apps/psr-desktop/src/main.ts",
                "polyfills": "apps/psr-desktop/src/polyfills.ts",
                "tsConfig": "apps/psr-desktop/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/psr-desktop/src/favicon.ico",
                    "apps/psr-desktop/src/assets"
                ],
                "styles": ["apps/psr-desktop/src/styles/index.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/psr-desktop/src/environments/environment.ts",
                            "with": "apps/psr-desktop/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "development"
        },
        "serve": {
            "executor": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "psr-desktop:build:production"
                },
                "development": {
                    "browserTarget": "psr-desktop:build:development",
                    "proxyConfig": "proxy.config.json"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "psr-desktop:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "apps/psr-desktop/src/**/*.ts",
                    "apps/psr-desktop/src/**/*.html"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/apps/psr-desktop"],
            "options": {
                "jestConfig": "apps/psr-desktop/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["scope:psr", "type:app", "platform:web"]
}
