{
    "projectType": "library",
    "root": "libs/config",
    "sourceRoot": "libs/config/src",
    "prefix": "psr-config",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:ng-packagr-lite",
            "outputs": ["dist/libs/config"],
            "options": {
                "project": "libs/config/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/config/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/config/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/config"],
            "options": {
                "jestConfig": "libs/config/jest.config.js",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "libs/config/src/**/*.ts",
                    "libs/config/src/**/*.html"
                ]
            }
        }
    },
    "tags": ["scope:config", "type:util", "platform:web"]
}
